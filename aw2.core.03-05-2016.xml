	<item>
		<title>edit-redirect</title>
		<link>http://localhost/nawesome/aw2_core/edit-redirect/</link>
		<pubDate>Sat, 22 Aug 2015 09:36:36 +0000</pubDate>
		<dc:creator><![CDATA[admin]]></dc:creator>
		<guid isPermaLink="false">http://localhost/wpex/?post_type=aw2_core&#038;p=4903</guid>
		<description></description>
		<content:encoded><![CDATA[[aw2_if_logged_in role='develop_for_awesomeui']
  [aw2_query_posts part=main run=true]
  	{
  		"posts_per_page": -1,
  		"name":"[aw2_request slug]",
  		"post_type": [ "page", "aw2_module" , "aw2_page", "aw2_core" , "aw2_query" , "aw2_component" , "aw2_hook" , "aw2_data" ],
  		"post_status": "publish"
  	}
  [/aw2_query_posts]
  [aw2_collection count set_param='results_count' /]
  [aw2_collection yes]  
    [aw2_if cond='{{aw2_param results_count}}' equal=1]
      [aw2_loop]
        [aw2_set_param post_id='{{aw2_loop_value ID}}']
      [/aw2_loop]  
      [aw2_script]
        document.location.href="[aw2_url site]/wp-admin/post.php?post=[aw2_param post_id]&action=edit"
      [/aw2_script]
    [/aw2_if]    
    [aw2_else]
      [aw2_loop]
        <div><a href="[aw2_url site]/wp-admin/post.php?post=[aw2_loop_value ID]&action=edit">[aw2_loop_value post_title]([aw2_loop_value post_type])</a></div>
      [/aw2_loop]  
    [/aw2_else]    
  [/aw2_collection]  
  [aw2_collection no]
    [aw2_request slug] Not found
  [/aw2_collection]  
[/aw2_if_logged_in]]]></content:encoded>
