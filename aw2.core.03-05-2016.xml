		<description></description>
		<content:encoded><![CDATA[<div  class="archive row">
	[aw2.loop wp_query.posts]
		<article class="[aw2.get function.get_post_class p1='element-item col-md-4 col-sm-6' separator=' ']"
		         id="post-[aw2.get item.ID]" >
		  <a class="aw-post-thumb"
		     href="[aw2.get item.url]">
			[aw2.get item.featured_image size="medium" /]
		  </a>
		  <div class="aw-post">
			<a class="aw-post-cat"
			   href="#">
			  [aw2.get item.taxonomy.name.category separator=","/]
			</a>
			<span class="aw-post-date pull-right">
			  [aw2.get item.post_date date_format='M d, Y']
			</span>
			<a class="aw-post-title"
			   href="[aw2.get item.url]">[aw2.get item.post_title /]</a>
			<div class="aw-author-name">
			  by  [aw2.get item.author_meta.user_nicename /]
			</div>
			<div class="aw-post-excerpt">
			  [aw2.get item.excerpt words=30  ellipsis=true /]
			</div>
		  </div>
		</article>
	[/aw2.loop]
</div>]]></content:encoded>
