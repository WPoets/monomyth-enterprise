	<item>
		<title>init</title>
		<link>http://localhost/nawesome/aw2_core/init__trashed/</link>
		<pubDate>Wed, 05 Aug 2015 04:44:14 +0000</pubDate>
		<dc:creator><![CDATA[admin]]></dc:creator>
		<guid isPermaLink="false">http://localhost/wpex/?post_type=aw2_core&#038;p=4857</guid>
		<description></description>
		<content:encoded><![CDATA[[aw2_set_template init]
[/aw2_set_template]  
[aw2_rewrite_rule]
{
  "regex":"z/(.+?)/?$",
  "redirect":"index.php?pagename=junk&slug=$matches[1]&z=true",
  "after":"top"
}
[/aw2_rewrite_rule]
[aw2_set_template query_vars]
	[aw2_query_vars z]
	[aw2_query_vars slug]
[/aw2_set_template] ]]></content:encoded>
